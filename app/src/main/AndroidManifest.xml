<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eascs.cwcharts">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.eascs.cwcharts.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".lib.ac.LineChartActivity"
            android:label="@string/title_activity_line_chart"></activity>
        <activity
            android:name=".lib.ac.ColumnChartActivity"
            android:label="@string/title_activity_column_chart"></activity>
        <activity
            android:name=".lib.ac.PieChartActivity"
            android:label="@string/title_activity_pie_chart"></activity>
        <activity
            android:name=".lib.ac.BubbleChartActivity"
            android:label="@string/title_activity_bubble_chart"></activity>
        <activity
            android:name=".lib.ac.PreviewLineChartActivity"
            android:label="@string/title_activity_preview_line_chart"></activity>
        <activity
            android:name=".lib.ac.PreviewColumnChartActivity"
            android:label="@string/title_activity_preview_column_chart"></activity>
        <activity
            android:name=".lib.ac.ComboLineColumnChartActivity"
            android:label="@string/title_activity_combo_line_column_chart"></activity>
        <activity
            android:name=".lib.ac.LineColumnDependencyActivity"
            android:label="@string/title_activity_line_column_dependency"></activity>
        <activity
            android:name=".lib.ac.GoodBadChartActivity"
            android:label="@string/title_activity_good_bad"></activity>
        <activity
            android:name=".lib.ac.TempoChartActivity"
            android:label="@string/title_activity_tempo_chart"></activity>
        <activity
            android:name=".lib.ac.SpeedChartActivity"
            android:label="@string/title_activity_speed_chart"></activity>
        <activity
            android:name=".lib.ac.ViewPagerChartsActivity"
            android:label="@string/title_activity_view_pager_charts"></activity>
        <activity
            android:name=".lib.ac.AboutActivity"
            android:label="@string/title_activity_about"></activity>
    </application>

</manifest>